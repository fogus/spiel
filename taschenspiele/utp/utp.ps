%%BeginProlog

/hw {25} bind def
/fw {hw 2 mul} bind def
/fh {hw 3 sqrt mul ceiling} bind def

/cell {
  newpath
  moveto
  fw 0 rmoveto
  hw neg fh rlineto
  fw neg 0 rlineto
  hw neg fh neg rlineto
  hw fh neg rlineto
  fw 0 rlineto
  closepath
} bind def

/column {
  dup fh mul 1 1 3 index {
    fh 2 mul mul 1 index sub 396 add 3 index 3 hw mul mul 306 add exch
    cell stroke
  } for
  pop pop pop
} bind def

/hexhex {
  dup 1 sub dup neg -1 exch {
    dup abs 2 index 2 mul 1 sub exch sub
    column
  } for
  pop
} bind def

%%EndProlog

%%Page: 0
/hw {12} bind def
8 hexhex

/Times-Roman findfont
12 scalefont
setfont

newpath
10 20 moveto
(Hexhex8: Copolymer, Havannah, Yodd) show

showpage


